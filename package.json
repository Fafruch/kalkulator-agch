{
  "name": "kalkulator-agh",
  "version": "1.0.0",
  "description": "Kalkulator obliczajacy ilosc punktow na studia I stopnia na Akademii Gorniczo-Hutniczej w Krakowie",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Fafruch/kalkulator-agh.git"
  },
  "author": "Filip Duczyminski <f.duczyminski@gmail.com>",
  "dependencies": {
    "bootstrap": "4.0.0-alpha.6",
    "gh-pages": "^1.0.0",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.4",
    "react-scripts": "1.0.14",
    "react-transition-group": "^1.2.0",
    "redux": "^3.7.2",
    "uuid": "^3.1.0"
  },
  "devDependencies": {
    "enzyme": "^3.1.0",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-react": "^5.0.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jest": "^20.0.3",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-node": "^4.2.3",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.1",
    "react-test-renderer": "^16.0.0"
  },
  "scripts": {
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist"
  }
}
